  "bugs": {
    "url": "https://github.com/inkOfPixel/react-shopify/issues"
  },
  "homepage": "https://github.com/inkOfPixel/react-shopify#readme",
  "dependencies": {
    "apollo-boost": "0.1.6",
    "flow-bin": "0.73.0",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "lodash": "4.17.10",
    "react": "16.4.0",
    "react-apollo": "2.1.4",
    "react-dom": "16.4.0",
    "react-scripts": "1.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
