    "babel-preset-react": "^6.24.1",
    "eslint": "4.19.1",
    "eslint-plugin-react": "7.9.1",
    "prettier": "1.13.4",
    "react-styleguidist": "7.0.16",
    "rimraf": "2.6.2",
    "rollup": "0.59.4",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "9.1.3",
