{
  "name": "react-shopify",
  "version": "0.4.0",
  "description": "An opinionated library of React components built upon the Shopify Storefront API",
  "main": "dist/react-shopify.js",
  "scripts": {
    "demo": "react-scripts start",
    "prebuild": "rimraf dist",
    "build": "webpack",
    "release": "yarn build && np --yolo",
    "test": "react-scripts test --env=jsdom",
    "flow": "flow",
    "docs": "styleguidist server",
    "docs:build": "styleguidist build",
    "version": "yarn docs:build && git add docs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkOfPixel/react-shopify.git"
  },
  "keywords": [
    "react",
    "shopify",
    "components",
    "storefront-api"
  ],
  "author": "Massimo De Marchi <massimo@inkofpixel.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inkOfPixel/react-shopify/issues"
  },
  "homepage": "https://inkofpixel.github.io/react-shopify/",
  "dependencies": {
    "apollo-boost": "0.1.6",
    "babel-runtime": "6.26.0",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "hoist-non-react-statics": "2.5.5",
    "lodash": "4.17.10",
    "memoize-one": "4.0.0",
    "react-apollo": "2.1.4"
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "eslint": "4.19.1",
    "eslint-plugin-react": "7.9.1",
    "flow-bin": "0.73.0",
    "np": "3.0.4",
    "prettier": "1.13.4",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-scripts": "1.1.4",
    "react-styleguidist": "7.0.16",
    "rimraf": "2.6.2",
    "styled-components": "3.3.2",
